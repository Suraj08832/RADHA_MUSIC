{
    "schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "pip install -r requirements.txt"
    },
    "deploy": {
        "startCommand": "python -m RADHA_MUSIC",
        "healthcheckPath": "/",
        "healthcheckTimeout": 100,
        "restartPolicyType": "ON_FAILURE",
        "numReplicas": 1
    },
    "variables": {
        "PYTHON_VERSION": "3.10.0"
    }
} 